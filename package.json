  "version": "0.0.1",
  "scripts": {
    "start": "react-scripts start",
    "prepare": "yarn build",
    "test": "yarn format && yarn lint",
    "format": "prettier -w src",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",

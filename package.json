    "react": "^18.2.0",
    "react-bootstrap": "^2.7.2",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.2.1",
    "react-redux": "^8.0.5"
  },
  "packageManager": "yarn@3.4.1",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bootstrap": "^5.2.6",
